# Wallet private key for deployment
OWNER_ADDRESS=your_owner_address_here
PRIVATE_KEY=your_private_key_here

# After deployment, add your contract's address here
CONTRACT_ADDRESS=your_contract_address_here

TOKEN_NAME="ABLE Token"
TOKEN_SYMBOL="ABLE"

# The initial token supply in wei (e.g., 1 billion tokens with 18 decimals)
INITIAL_SUPPLY=1000000000000000000000000000

# Base RPC endpoints
BASE_MAINNET_RPC=https://mainnet.base.org
BASE_SEPOLIA_RPC=https://sepolia.base.org

# Etherscan or BaseScan API key
ETHERSCAN_API_KEY=your_basescan_api_key_here

# CoinMarketCap API key
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# Flag to report gas usage in tests
REPORT_GAS=true